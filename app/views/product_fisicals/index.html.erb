<p id="notice"><%= notice %></p>

<h1>Product Fisicals</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Size</th>
      <th>Description</th>
      <th>Stock</th>
      <th>Price</th>
      <th>Sku</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_fisicals.each do |product_fisical| %>
      <tr>
        <td><%= product_fisical.name %></td>
        <td><%= product_fisical.size %></td>
        <td><%= product_fisical.description %></td>
        <td><%= product_fisical.stock %></td>
        <td><%= product_fisical.price %></td>
        <td><%= product_fisical.sku %></td>
        <td><%= link_to 'Show', product_fisical %></td>
        <td><%= link_to 'Edit', edit_product_fisical_path(product_fisical) %></td>
        <td><%= link_to 'Destroy', product_fisical, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Fisical', new_product_fisical_path %>
